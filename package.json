{
  "name": "pubg-buddy",
  "version": "1.0.0",
  "description": "A bot that reports PUBG stats",
  "main": "src/index.ts",
  "author": "Thomas Ortiz",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node node --optimize_for_size --max_old_space_size=460 dist/index.js",
