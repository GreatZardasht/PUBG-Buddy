  },
  "dependencies": {
    "axios": "^0.18.0",
    "dblapi.js": "^2.3.0",
    "discord.js": "^11.4.2",
    "dotenv": "^6.1.0",
    "jimp": "^0.5.6",
    "mixpanel": "^0.9.2",
    "node-cache": "^4.2.0",
    "pg": "^7.6.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/mixpanel": "^2.14.0",
    "@types/node": "^10.12.5",
    "@types/pg": "^7.4.11",
    "eslint": "^5.9.0",
    "typescript": "^3.1.6"
  }
}
